@{
    Layout = "~/Views/Shared/_layout.cshtml";
}
<table border="1">
    <thead>
    <td>Name</td>
    <td>Surname</td>
    <td>Age</td>
    <td>Country</td>
    <td>PhoneNumber</td>
    </thead>
    <tbody>
    <td>@ViewBag.Name</td>
    <td>@ViewBag.Surname</td>
    <td>@ViewBag.Age</td>
    <td>@ViewBag.Country</td>
    <td>@ViewBag.PhoneNumber</td>
    </tbody>
</table>
